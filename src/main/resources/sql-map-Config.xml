<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
		"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings lazyLoadingEnabled="true" maxRequests="20"
		maxSessions="10" maxTransactions="50" useStatementNamespaces="true"
		statementCachingEnabled="false" />

	<sqlMap resource="Department.xml" />
	<sqlMap resource="Person.xml" />
	<sqlMap resource="Job.xml" />
	<sqlMap resource="MultiDay.xml" />
	<sqlMap resource="Client.xml" />
	<sqlMap resource="ClientPrincipal.xml" />
	<sqlMap resource="Salary.xml" />

	<sqlMap resource="Authority.xml" />
	<sqlMap resource="User.xml" />
	<sqlMap resource="OperateLog.xml" />

	<sqlMap resource="Project.xml" />
	<sqlMap resource="BuildingProject.xml" />
	<sqlMap resource="BuildingProjectDetail.xml" />
	<sqlMap resource="Dispatch.xml" />
	<sqlMap resource="DispatchWork.xml" />
	<sqlMap resource="Quote.xml" />
	<sqlMap resource="QuoteItem.xml" />

	<sqlMap resource="PurchaseItem.xml" />
	<sqlMap resource="Purchase.xml" />
</sqlMapConfig>