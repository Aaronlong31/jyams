/*主导航菜单*/
#menu ul {padding:0;border:0;list-style:none;margin-top: 0;margin-right: 0;margin-bottom: 0;margin-left: 15px;}
#menu {background:url(../images/menu_bg.gif) repeat-x;height:73px;z-index:1;}
.menu_line {background:url(../images/menu_line.gif) no-repeat center top;width:8px;}
.menu_line2 {background:url(../images/menu_line2.gif) no-repeat center top;	width:15px;}
.menu_line3 {background:url(../images/menu_line3.gif) no-repeat center top;width:8px;}
#nav {padding-left:20px;}
#nav li {float:left;height:42px;}
#nav li a {float:left;display:block;padding-left:6px;height:41px;background:url(../images/menu_on_left.gif) no-repeat left top;cursor:pointer;text-decoration:none;}
#nav li a span {float:left;padding:12px 14px 10px 5px;background:url(../images/menu_on_right.gif) no-repeat right top;font-size:14px;color:#fff;text-decoration:none;height:19px;}
#nav li a img{ float:left; margin-top:12px; margin-left:10px}
#nav li .nav_on { background-position:left 100%;}
#nav li .nav_on span {background-position:right 100%;color:#328DDE;text-decoration:none;padding:12px 14px 10px 5px;;font-weight:bold;}
#nav li .nav_on img{ margin-top:15px;}
#menu_con {text-align:left;padding-left:20px;clear:both; margin-top:2px; float:left}
#menu_con li {float:left;height:22px;margin-top:1px;}
#menu_con li a {display:block;float:left;background:url(../images/menu_on_left2.gif) no-repeat left top;cursor:pointer;	padding-left:3px;text-decoration:none; color:#fff}
#menu_con li a span {float:left;padding:6px 10px 4px 10px;line-height:12px;background:url(../images/menu_on_right2.gif) no-repeat right top;color:#000;}
#menu_con li a:hover {text-decoration:none;background:url(../images/menu_on_left2.gif) no-repeat left bottom;}
#menu_con li a:hover span {background:url(../images/menu_on_right2.gif) no-repeat right bottom;color:#449ED4}
#menu_con li a.select {text-decoration:none;background:url(../images/menu_on_left1.gif) no-repeat left bottom;}
#menu_con li a.select span {background:url(../images/menu_on_right1.gif) no-repeat right bottom;color:#000}


